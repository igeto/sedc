@model NewMVCProject.Models.StudentViewModel
@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>

@using (Html.BeginForm())
{
    <div>First Name:</div>@Html.TextBoxFor(x => x.FirstName)
