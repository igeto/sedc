        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Label x:Name="lblName" Content="Enter name" HorizontalAlignment="Left" Margin="46,40,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblResult" Content="Label" HorizontalAlignment="Left" Margin="72,165,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnPrint" Content="Print" HorizontalAlignment="Left" Margin="360,210,0,0" VerticalAlignment="Top" Width="75" Click="btnPrint_Click"/>
        <TextBox x:Name="textBoxName" HorizontalAlignment="Left" Height="23" Margin="130,43,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="212"/>

    </Grid>
</Window>
